<div class="container">
  <% if flash[:notice] %>
  <%= flash[:notice] %>
  <%end %>

</div>
<div class="container">
  <div class='col-md-2'></div>
  <div class="col-md-6">
  <div class="row">
    <div class="login-box well">
<%= form_for(:user, url: loggedin_path, method: :get) do |f| %>

  <div class="form-group">
    <%= f.label :Email %>
    <%= f.email_field :email, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :Password %>
    <%= f.password_field :password, class: 'form-control' %>
  </div>
  <%= f.submit "Log in", class: "btn btn-primary center-block" %>
<% end %>
</div>
</div>
</div>
</div>